{
  "name": "Test extension",
  "description": "gm",
  "version": "0.1",
  "manifest_version": 3,

  "background": {
    "service_worker": "./service/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./img/icon16.png",
      "48": "./img/icon48.png",
      "128": "./img/icon128.png"
    }
  },
  "icons": {
    "16": "./img/icon16.png",
    "48": "./img/icon48.png",
    "128": "./img/icon128.png"
  }
}
